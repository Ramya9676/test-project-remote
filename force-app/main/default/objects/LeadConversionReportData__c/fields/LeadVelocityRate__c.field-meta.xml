<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LeadVelocityRate__c</fullName>
    <description>This field calculates the percentage of lead conversion compared to last month and the current month</description>
    <formula>((CurrentMonthAmount__c -  LastMonthAmount__c ) /  LastMonthAmount__c )</formula>
    <label>LeadVelocityRate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
