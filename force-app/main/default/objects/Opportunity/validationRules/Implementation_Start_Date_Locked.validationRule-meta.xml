<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Implementation_Start_Date_Locked</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT (ISBLANK(Implementation_Closed_Date__c) ),
			
  		 ISCHANGED(Implementation_Start_Date__c) 	)</errorConditionFormula>
    <errorDisplayField>Implementation_Start_Date__c</errorDisplayField>
    <errorMessage>Implementation start date can&apos;t be modified after Implementation close date entered</errorMessage>
</ValidationRule>
